{
  "name": "@ckeditor/ckeditor5-vue",
  "version": "7.3.0",
  "description": "Official Vue.js 3+ component for CKEditor 5 – the best browser-based rich text editor.",
  "keywords": [
    "wysiwyg",
    "rich text",
    "editor",
    "html",
    "contentEditable",
    "editing",
    "vue",
    "vue.js",
    "vue component",
    "vue.js component",
    "ckeditor",
    "ckeditor5",
    "ckeditor 5"
  ],
  "type": "module",
  "main": "./dist/ckeditor.umd.cjs",
  "module": "./dist/ckeditor.js",
  "types": "./dist/plugin.d.ts",
  "exports": {
    ".": {
      "types": "./dist/plugin.d.ts",
      "import": "./dist/ckeditor.js",
      "require": "./dist/ckeditor.umd.cjs"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-integrations-common": "^2.2.2",
    "lodash-es": "^4.17.21"
  },
  "peerDependencies": {
    "ckeditor5": ">=42.0.0 || ^0.0.0-nightly",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@ckeditor/ckeditor5-dev-bump-year": "^54.2.0",
    "@ckeditor/ckeditor5-dev-changelog": "^54.2.0",
    "@ckeditor/ckeditor5-dev-ci": "^54.2.0",
    "@ckeditor/ckeditor5-dev-release-tools": "^54.2.0",
    "@ckeditor/ckeditor5-dev-utils": "^54.2.0",
    "@types/lodash-es": "^4.17.6",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitest/browser": "^2.1.9",
    "@vitest/coverage-istanbul": "^2.1.4",
    "@vitest/ui": "^2.1.4",
    "@vue/test-utils": "^2.3.1",
    "ckeditor5": "^45.0.0",
    "ckeditor5-premium-features": "^45.0.0",
    "eslint": "^9.38.0",
    "eslint-config-ckeditor5": "^12.2.0",
    "eslint-plugin-ckeditor5-rules": "^12.2.0",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.1.0",
    "husky": "^8.0.2",
    "lint-staged": "^10.2.11",
    "listr2": "^6.5.0",
    "minimist": "^1.2.5",
    "semver": "^7.0.0",
    "typescript": "~5.4.5",
    "typescript-eslint": "^8.32.1",
    "upath": "^2.0.1",
    "vite": "^5.4.21",
    "vitest": "^2.1.9",
    "vue": "^3.4.30",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^2.1.6",
    "webdriverio": "^9.12.7"
  },
  "engines": {
    "node": ">=24.11.0",
    "pnpm": ">=10.14.0",
    "yarn": "\n\n┌─────────────────────────┐\n│  Hey, we use pnpm now!  │\n└─────────────────────────┘\n\n"
  },
  "scripts": {
    "nice": "ckeditor5-dev-changelog-create-entry",
    "dev": "vite",
    "build": "tsc --noEmit && vite build && vue-tsc --emitDeclarationOnly",
    "test": "vitest run --coverage",
    "test:watch": "vitest --ui --watch",
    "test:check:types": "tsc --noEmit -p ./tests/tsconfig.json",
    "lint": "eslint",
    "postinstall": "node ./scripts/postinstall.js",
    "release:prepare-changelog": "node ./scripts/preparechangelog.js",
    "release:prepare-packages": "node ./scripts/preparepackages.js",
    "release:publish-packages": "node ./scripts/publishpackages.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ckeditor/ckeditor5-vue.git"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE.md"
  ],
  "author": "CKSource (http://cksource.com/)",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/ckeditor/ckeditor5-vue/issues"
  },
  "homepage": "https://github.com/ckeditor/ckeditor5-vue",
  "lint-staged": {
    "**/*": [
      "eslint --quiet"
    ]
  }
}
